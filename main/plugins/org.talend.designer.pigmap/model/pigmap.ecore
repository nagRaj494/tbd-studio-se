  <eClassifiers xsi:type="ecore:EClass" name="PigMapData" eSuperTypes="../../org.talend.model/model/TalendFile.ecore#//AbstractExternalData">
    <eStructuralFeatures xsi:type="ecore:EReference" name="inputTables" upperBound="-1"
