    <eStructuralFeatures xsi:type="ecore:EReference" name="source" eType="#//AbstractNode"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="target" eType="#//AbstractInOutTable"/>
  </eClassifiers>
</ecore:EPackage>
