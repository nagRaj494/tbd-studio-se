<COMPONENT>
	<HEADER PLATEFORM="ALL" SERIAL="" VERSION="0.101" STATUS="ALPHA"
		COMPATIBILITY="ALL" AUTHOR="Talend" RELEASE_DATE="20070210A"
		STARTABLE="true">
		<SIGNATURE />
	</HEADER>

	<FAMILIES>
		<FAMILY>Internet/MapR Streams</FAMILY>
	</FAMILIES>

	<DOCUMENTATION>
		<URL />
	</DOCUMENTATION>

	<CONNECTORS>
		<CONNECTOR CTYPE="FLOW" MAX_INPUT="0" MAX_OUTPUT="1" />
		<CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" MAX_OUTPUT="1" />
		<CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
		<CONNECTOR CTYPE="COMPONENT_OK" />
		<CONNECTOR CTYPE="COMPONENT_ERROR" />
		<CONNECTOR CTYPE="RUN_IF" />
	</CONNECTORS>

	<PARAMETERS>

		<PARAMETER NAME="OUTPUT_TYPE" FIELD="CLOSED_LIST" SHOW="true"
			NUM_ROW="5">
			<ITEMS DEFAULT="STRING">
				<ITEM NAME="STRING" VALUE="STRING" />
				<ITEM NAME="BYTES" VALUE="BYTES" />
			</ITEMS>
		</PARAMETER>

		<PARAMETER NAME="SCHEMA" FIELD="SCHEMA_TYPE" NUM_ROW="5"
			REQUIRED="true">
			<TABLE IF="(OUTPUT_TYPE == 'BYTES') AND (SAVE_OFFSET == 'true')"
				READONLY="true">
				<COLUMN NAME="payload" TYPE="id_byte[]" READONLY="true"
					CUSTOM="true" />
				<COLUMN NAME="offset" TYPE="id_Long" READONLY="true" CUSTOM="true" />
			</TABLE>
			<TABLE IF="(OUTPUT_TYPE == 'BYTES') AND (SAVE_OFFSET == 'false')"
				READONLY="true">
				<COLUMN NAME="payload" TYPE="id_byte[]" READONLY="true"
					CUSTOM="true" />
			</TABLE>
			<TABLE IF="(OUTPUT_TYPE == 'STRING') AND (SAVE_OFFSET == 'true')"
				READONLY="true">
				<COLUMN NAME="payload" TYPE="id_String" READONLY="true"
					CUSTOM="true" />
				<COLUMN NAME="offset" TYPE="id_Long" READONLY="true" CUSTOM="true" />
			</TABLE>
			<TABLE IF="(OUTPUT_TYPE == 'STRING') AND (SAVE_OFFSET == 'false')"
				READONLY="true">
				<COLUMN NAME="payload" TYPE="id_String" READONLY="true"
					CUSTOM="true" />
			</TABLE>
		</PARAMETER>

		<PARAMETER NAME="USE_EXISTING_CONNECTION" FIELD="CHECK"
			NUM_ROW="10">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="CONNECTION" FIELD="COMPONENT_LIST"
			REQUIRED="true" FILTER="tMapRStreamsConnection" NUM_ROW="10"
			SHOW_IF="USE_EXISTING_CONNECTION == 'true'" />

		<PARAMETER NAME="MAPRSTREAMS" FIELD="HADOOP_DISTRIBUTION"
			NUM_ROW="20" REQUIRED="false" SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
			GROUP="VERSION" />

		<PARAMETER NAME="STREAM_TOPIC" FIELD="TEXT" REQUIRED="true"
			NUM_ROW="40" NB_LINES="3">
			<DEFAULT>"/mystream:mytopic"</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="GROUP_ID" FIELD="TEXT" NUM_ROW="50">
			<DEFAULT>"mygroup"</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="RESET_OFFSET" FIELD="CHECK" NUM_ROW="60"
			GROUP="OFFSET_MANAGEMENT_GROUP">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="AUTO_OFFSET_RESET" FIELD="CLOSED_LIST"
			NUM_ROW="71" GROUP="OFFSET_MANAGEMENT_GROUP">
			<ITEMS DEFAULT="LATEST">
				<ITEM NAME="LATEST" VALUE="LATEST" />
				<ITEM NAME="EARLIEST" VALUE="EARLIEST" />
			</ITEMS>
		</PARAMETER>

		<PARAMETER NAME="AUTO_COMMIT_OFFSET" FIELD="CHECK" NUM_ROW="90"
			GROUP="OFFSET_MANAGEMENT_GROUP">
			<DEFAULT>true</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="COMMIT_INTERVAL" FIELD="TEXT" NUM_ROW="90"
			REQUIRED="true" SHOW_IF="(AUTO_COMMIT_OFFSET == 'true')" GROUP="OFFSET_MANAGEMENT_GROUP">
			<DEFAULT>5000</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="USE_BATCH_MAX_DURATION" FIELD="CHECK"
			NUM_ROW="100" GROUP="BATCH_MANAGEMENT_GROUP">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="BATCH_MAX_DURATION" FIELD="TEXT" NUM_ROW="100"
			REQUIRED="true" SHOW_IF="(USE_BATCH_MAX_DURATION == 'true')" GROUP="BATCH_MANAGEMENT_GROUP">
			<DEFAULT>600000</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="USE_BATCH_MAX_SIZE" FIELD="CHECK" NUM_ROW="110"
			GROUP="BATCH_MANAGEMENT_GROUP">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="BATCH_MAX_SIZE" FIELD="TEXT" NUM_ROW="110"
			REQUIRED="true" SHOW_IF="(USE_BATCH_MAX_SIZE == 'true')" GROUP="BATCH_MANAGEMENT_GROUP">
			<DEFAULT>5000</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="USE_BATCH_MESSAGE_TIMEOUT" FIELD="CHECK"
			NUM_ROW="120" GROUP="BATCH_MANAGEMENT_GROUP">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="BATCH_MESSAGE_TIMEOUT" FIELD="TEXT"
			NUM_ROW="120" REQUIRED="true" SHOW_IF="(USE_BATCH_MESSAGE_TIMEOUT == 'true')"
			GROUP="BATCH_MANAGEMENT_GROUP">
			<DEFAULT>10000</DEFAULT>
		</PARAMETER>

	</PARAMETERS>

	<ADVANCED_PARAMETERS>
		<PARAMETER NAME="CONSUMER_PROPERTIES" FIELD="TABLE"
			REQUIRED="false" NUM_ROW="40" NB_LINES="3">
			<ITEMS>
				<ITEM NAME="PROPERTY" />
				<ITEM NAME="VALUE" />
			</ITEMS>
		</PARAMETER>

		<PARAMETER NAME="CONSUMER_TIMEOUT" FIELD="TEXT" NUM_ROW="42"
			REQUIRED="true">
			<DEFAULT>-1</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="SAVE_OFFSET" FIELD="CHECK" NUM_ROW="45"
			REQUIRED="true">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="CUSTOM_ENCODING" FIELD="CHECK" NUM_ROW="50"
			REQUIRED="true">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="ENCODING" FIELD="ENCODING_TYPE" NUM_ROW="51"
			REQUIRED="true" SHOW_IF="CUSTOM_ENCODING == 'true'">
			<DEFAULT>"UTF-8"</DEFAULT>
		</PARAMETER>
	</ADVANCED_PARAMETERS>

	<CODEGENERATION />
	<RETURNS />
</COMPONENT>
