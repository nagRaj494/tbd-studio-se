			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="DBNAME" FIELD="TEXT" NUM_ROW="30"
			REPOSITORY_VALUE="SID" REQUIRED="true" GROUP="CONNECTION"
			SHOW_IF="USE_EXISTING_CONNECTION=='false'">
			<DEFAULT>"default"</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="USE_KRB" FIELD="CHECK" NUM_ROW="35"
			REQUIRED="true" REPOSITORY_VALUE="USE_KRB" GROUP="AUTHENTICATION"
			SHOW_IF="USE_EXISTING_CONNECTION=='false'">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="IMPALA_PRINCIPAL" FIELD="TEXT" NUM_ROW="40"
			REQUIRED="true" REPOSITORY_VALUE="IMPALA_PRINCIPAL" GROUP="AUTHENTICATION"
			SHOW_IF="(isShow[USE_KRB]) AND USE_KRB=='true'">
			<DEFAULT>"impala/_HOST@EXAMPLE.COM"</DEFAULT>
		</PARAMETER>

		<!-- CONNECTION PART END -->

		<!-- WRITE PART BEGIN -->
		<PARAMETER NAME="SCHEMA" FIELD="SCHEMA_TYPE" NUM_ROW="60">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="TABLE" FIELD="DBTABLE" NUM_ROW="65">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="ACTION" FIELD="CLOSED_LIST" NUM_ROW="70">
			<ITEMS DEFAULT="INTO">
				<ITEM NAME="OVERWRITE" VALUE="OVERWRITE" />
				<ITEM NAME="APPEND" VALUE="INTO" />
			</ITEMS>
		</PARAMETER>

		<PARAMETER NAME="EXTENDINSERT" FIELD="CHECK" NUM_ROW="75">
			<DEFAULT>true</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="NB_ROWS_PER_INSERT" FIELD="TEXT" NUM_ROW="75"
			REQUIRED="true" SHOW_IF="(EXTENDINSERT == 'true')">
			<DEFAULT>100</DEFAULT>
		</PARAMETER>
		<!-- WRITE PART END -->

		<PARAMETER NAME="MAPPING" FIELD="MAPPING_TYPE" NUM_ROW="10"
			SHOW="false">
			<DEFAULT>hive_id</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="DIE_ON_ERROR" FIELD="CHECK" NUM_ROW="80">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
