<COMPONENT>
    <HEADER
        AUTHOR="Talend"
        COMPATIBILITY="ALL"
        DATA_AUTO_PROPAGATE="true"
        PLATEFORM="ALL"
        RELEASE_DATE="20080229A"
        SERIAL=""
        STATUS="ALPHA"
        VERSION="0.102"
        STARTABLE="false"
    >
        <SIGNATURE />
    </HEADER>
    <FAMILIES>
        <FAMILY>Databases/Cassandra</FAMILY>
        <FAMILY>Big Data/Cassandra</FAMILY>
    </FAMILIES>
    <DOCUMENTATION>
        <URL />
    </DOCUMENTATION>
    <CONNECTORS>
        <CONNECTOR CTYPE="FLOW" MAX_INPUT="1" MAX_OUTPUT="0"/>
        <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="0" MAX_INPUT="0"/>
        <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
        <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
        <CONNECTOR CTYPE="COMPONENT_OK"/>
        <CONNECTOR CTYPE="COMPONENT_ERROR"/>
        <CONNECTOR CTYPE="RUN_IF"/>
    </CONNECTORS>
    <PARAMETERS>
        <PARAMETER NAME="PROPERTY" FIELD="PROPERTY_TYPE" NUM_ROW="1" REPOSITORY_VALUE="NOSQL:CASSANDRA"/>
        <PARAMETER
            NAME="NOTE"
            FIELD="LABEL"
            NUM_ROW="2"
            COLOR="255;0;0"
            SHOW_IF="DB_VERSION=='CASSANDRA_1_2_2'"
        >
            <DEFAULT>This component requires the server OS and the client OS to be similar.</DEFAULT>
        </PARAMETER>
        <PARAMETER
            NAME="NOTE"
            FIELD="LABEL"
            NUM_ROW="2"
            COLOR="255;0;0"
            SHOW_IF="DB_VERSION=='CASSANDRA_1_1_2'"
        >
            <DEFAULT>This component only works locally on a Linux OS.</DEFAULT>
        </PARAMETER>
        <PARAMETER
            NAME="SCHEMA_FLOW"
            FIELD="SCHEMA_TYPE"
            NUM_ROW="3"
        >
            <DEFAULT>""</DEFAULT>
        </PARAMETER>
        <PARAMETER
            NAME="MAPPING"
            FIELD="MAPPING_TYPE"
            NUM_ROW="3"
            SHOW="false"
        >
            <DEFAULT IF="(DB_VERSION == 'CASSANDRA_1_1_2') OR (DB_VERSION == 'CASSANDRA_1_2_2') OR (DB_VERSION == 'CASSANDRA_2_0_0')">cassandra_id</DEFAULT>
            <DEFAULT IF="(DB_VERSION == 'CASSANDRA_2_1')">cassandra_datastax_id</DEFAULT>
            <DEFAULT IF="(DB_VERSION == 'CASSANDRA_2_2') OR (DB_VERSION == 'CASSANDRA_3_0')">cassandra22_datastax_id</DEFAULT>
        </PARAMETER>
        <!-- AFTER 2.1 only keep CQL type -->
        <PARAMETER
            NAME="TABLE_TYPE"
            FIELD="CLOSED_LIST"
            NUM_ROW="4"
            SHOW_IF="(DB_VERSION == 'CASSANDRA_2_0_0')"
        >
            <ITEMS DEFAULT="NON_CQL">
                <ITEM NAME="NON_CQL" VALUE="NON_CQL"/>
                <ITEM NAME="CQL" VALUE="CQL"/>
            </ITEMS>
        </PARAMETER>
        <PARAMETER
            NAME="DB_VERSION"
            FIELD="CLOSED_LIST"
            REPOSITORY_VALUE="DB_VERSION"
            NUM_ROW="5"
            >
            <ITEMS DEFAULT="CASSANDRA_3_0">
                <ITEM NAME="CASSANDRA_1_1_2" VALUE="CASSANDRA_1_1_2"/>
                <ITEM NAME="CASSANDRA_1_2_2" VALUE="CASSANDRA_1_2_2"/>
                <ITEM NAME="CASSANDRA_2_0_0" VALUE="CASSANDRA_2_0_0"/>
                <ITEM NAME="CASSANDRA_2_1" VALUE="CASSANDRA_2_1"/>
                <ITEM NAME="CASSANDRA_2_2" VALUE="CASSANDRA_2_2"/>
                <ITEM NAME="CASSANDRA_3_0" VALUE="CASSANDRA_3_0"/>
            </ITEMS>
        </PARAMETER>
        <PARAMETER
            NAME="HOST"
            FIELD="TEXT"
            REPOSITORY_VALUE="HOST"
            REQUIRED="true"
            NUM_ROW="10"
        >
            <DEFAULT>""</DEFAULT>
        </PARAMETER>
        <PARAMETER
            NAME="PORT"
            FIELD="TEXT"
            REPOSITORY_VALUE="PORT"
            REQUIRED="true"
            NUM_ROW="10"
        >
            <DEFAULT IF="(DB_VERSION == 'CASSANDRA_1_1_2') OR (DB_VERSION == 'CASSANDRA_1_2_2') OR (DB_VERSION == 'CASSANDRA_2_0_0') OR (DB_VERSION == 'CASSANDRA_2_1')">9160</DEFAULT>
            <DEFAULT IF="(DB_VERSION == 'CASSANDRA_2_2') OR (DB_VERSION == 'CASSANDRA_3_0')">9042</DEFAULT>
        </PARAMETER>
        <PARAMETER
            NAME="REQUIRED_AUTHENTICATION"
            FIELD="CHECK"
            REPOSITORY_VALUE="REQUIRED_AUTHENTICATION"
            NUM_ROW="15"
            SHOW_IF="DB_VERSION != 'CASSANDRA_1_1_2'"
        >
            <DEFAULT>false</DEFAULT>
        </PARAMETER>
        <PARAMETER
            NAME="USERNAME"
            FIELD="TEXT"
            REPOSITORY_VALUE="USERNAME"
            REQUIRED="true"
            NUM_ROW="20"
            SHOW_IF="(isShow[REQUIRED_AUTHENTICATION]) AND (REQUIRED_AUTHENTICATION == 'true')"
        >
            <DEFAULT>""</DEFAULT>
        </PARAMETER>
        <PARAMETER
            NAME="PASSWORD"
            FIELD="PASSWORD"
            REPOSITORY_VALUE="PASSWORD"
            REQUIRED="true"
            NUM_ROW="20"
            SHOW_IF="(isShow[REQUIRED_AUTHENTICATION]) AND (REQUIRED_AUTHENTICATION == 'true')"
        >
            <DEFAULT>""</DEFAULT>
        </PARAMETER>
        <PARAMETER NAME="USE_HTTPS" FIELD="CHECK" NUM_ROW="22" SHOW="false">
            <DEFAULT>false</DEFAULT>
        </PARAMETER>
        <PARAMETER NAME="HTTPS_SETTING" FIELD="COMPONENT_LIST" FILTER="tSetKeystore"
            NUM_ROW="22" SHOW_IF="(isShow[USE_HTTPS]) AND (USE_HTTPS == 'true')"
            DYNAMIC_SETTINGS="true" />
        <PARAMETER NAME="USE_CONF_FILE" FIELD="CHECK" NUM_ROW="23">
            <DEFAULT>false</DEFAULT>
        </PARAMETER>            
        <PARAMETER
            NAME="CONF_FILE"
            FIELD="FILE"
            REQUIRED="true"
            NUM_ROW="23"
            SHOW_IF="USE_CONF_FILE == 'true'"
        >
            <DEFAULT>""</DEFAULT>
        </PARAMETER> 
        <PARAMETER
            NAME="KEY_SPACE"
            FIELD="TEXT"
            REPOSITORY_VALUE="KEY_SPACE"
            REQUIRED="true"
            NUM_ROW="25"
        >
            <DEFAULT>""</DEFAULT>
        </PARAMETER>
        <PARAMETER
            NAME="COLUMN_FAMILY"
            FIELD="TEXT"
            REQUIRED="true"
            NUM_ROW="25"
        >
            <DEFAULT>""</DEFAULT>
        </PARAMETER>
        <PARAMETER
            NAME="PARTITIONER"
            FIELD="CLOSED_LIST"
            REQUIRED="true"
            NUM_ROW="25"
        >
            <ITEMS DEFAULT="RANDOMPARTITIONER">
                <ITEM NAME="RANDOMPARTITIONER" VALUE="RandomPartitioner"/>
                <ITEM NAME="MURMUR3PARTITIONER" VALUE="Murmur3Partitioner" NOT_SHOW_IF="DB_VERSION == 'CASSANDRA_1_1_2'"/>
                <ITEM NAME="ORDERPRESERVINGPARTITIONER" VALUE="OrderPreservingPartitioner"/>
            </ITEMS>
        </PARAMETER>
        <PARAMETER
            NAME="COMPARATOR"
            FIELD="CLOSED_LIST"
            REQUIRED="true"
            NUM_ROW="30"
            SHOW_IF="(DB_VERSION == 'CASSANDRA_1_1_2') OR (DB_VERSION == 'CASSANDRA_1_2_2') OR ((DB_VERSION == 'CASSANDRA_2_0_0') AND (TABLE_TYPE=='NON_CQL'))"
        >
            <ITEMS DEFAULT="ASCIITYPE">
                <ITEM NAME="ASCIITYPE" VALUE="AsciiType"/>
                <ITEM NAME="BYTESTYPE" VALUE="BytesType"/>
                <ITEM NAME="LEXICALUUIDTYPE" VALUE="LexicalUUIDType"/>
                <ITEM NAME="LONGTYPE" VALUE="LongType"/>
                <ITEM NAME="TIMEUUIDTYPE" VALUE="TimeUUIDType"/>
                <ITEM NAME="UTF8TYPE" VALUE="UTF8Type"/>
            </ITEMS>
        </PARAMETER>
        <!--PARAMETER
            NAME="SUBCOMPARATOR"
            FIELD="CLOSED_LIST"
            REQUIRED="true"
            NUM_ROW="25"
        >
            <ITEMS DEFAULT="NONE">
                <ITEM NAME="NONE" VALUE="NONE"/>
                <ITEM NAME="ASCIITYPE" VALUE="ASCIITYPE"/>
            </ITEMS>
        </PARAMETER-->
        <PARAMETER
            NAME="SCHEMA_STMT"
            FIELD="TEXT"
            NUM_ROW="26"
            REQUIRED="true"
            NOT_SHOW_IF="(DB_VERSION == 'CASSANDRA_1_1_2') OR (DB_VERSION == 'CASSANDRA_1_2_2') OR ((DB_VERSION == 'CASSANDRA_2_0_0') AND (TABLE_TYPE=='NON_CQL'))"
        >
          <DEFAULT>"create table ks.tb (id int, name text, birthday timestamp, primary key(id, birthday)) with clustering order by (birthday desc)"</DEFAULT>
        </PARAMETER>
        <PARAMETER
            NAME="INSERT_STMT"
            FIELD="TEXT"
            NUM_ROW="27"
            REQUIRED="true"
            NOT_SHOW_IF="(DB_VERSION == 'CASSANDRA_1_1_2') OR (DB_VERSION == 'CASSANDRA_1_2_2') OR ((DB_VERSION == 'CASSANDRA_2_0_0') AND (TABLE_TYPE=='NON_CQL'))"
        >
          <DEFAULT>"insert into ks.tb (id, name, birthday) values (?, ?, ?)"</DEFAULT>
        </PARAMETER>
        <PARAMETER
            NAME="SSTABLE_STORE_PATH"
            FIELD="DIRECTORY"
            REQUIRED="true"
            NUM_ROW="40"
        >
            <DEFAULT>""</DEFAULT>
        </PARAMETER>
        <PARAMETER
            NAME="BUFFER_SIZE"
            FIELD="TEXT"
            REQUIRED="true"
            NUM_ROW="45"
        >
            <DEFAULT IF="(DB_VERSION == 'CASSANDRA_1_1_2') OR (DB_VERSION == 'CASSANDRA_1_2_2') OR ((DB_VERSION == 'CASSANDRA_2_0_0') AND (TABLE_TYPE=='NON_CQL'))">64</DEFAULT>
            <DEFAULT IF="((DB_VERSION == 'CASSANDRA_2_0_0') AND (TABLE_TYPE=='CQL')) OR (DB_VERSION == 'CASSANDRA_2_1') OR (DB_VERSION == 'CASSANDRA_2_2') OR (DB_VERSION == 'CASSANDRA_3_0')">128</DEFAULT>
        </PARAMETER>
    </PARAMETERS>
    <ADVANCED_PARAMETERS/>
    <CODEGENERATION>
        <TEMPLATES INPUT="tCOB" OUTPUT="tCBE">
            <TEMPLATE NAME="tCOB" COMPONENT="tCassandraOutputBulk">
              <LINK_TO NAME="tCBE" CTYPE="ROWS_END"/>
            </TEMPLATE>

            <TEMPLATE NAME="tCBE" COMPONENT="tCassandraBulkExec"/>

            <TEMPLATE_PARAM SOURCE="self.SCHEMA_FLOW" TARGET="tCOB.SCHEMA_FLOW"/>
            <TEMPLATE_PARAM SOURCE="self.DB_VERSION" TARGET="tCOB.DB_VERSION"/>
            <TEMPLATE_PARAM SOURCE="self.KEY_SPACE" TARGET="tCOB.KEY_SPACE"/>
            <TEMPLATE_PARAM SOURCE="self.COLUMN_FAMILY" TARGET="tCOB.COLUMN_FAMILY"/>
            <TEMPLATE_PARAM SOURCE="self.PARTITIONER" TARGET="tCOB.PARTITIONER"/>
            <TEMPLATE_PARAM SOURCE="self.COMPARATOR" TARGET="tCOB.COMPARATOR"/>
            <TEMPLATE_PARAM SOURCE="self.BUFFER_SIZE" TARGET="tCOB.BUFFER_SIZE"/>
            <TEMPLATE_PARAM SOURCE="self.SSTABLE_STORE_PATH" TARGET="tCOB.SSTABLE_STORE_PATH"/>
            <TEMPLATE_PARAM SOURCE="self.TABLE_TYPE" TARGET="tCOB.TABLE_TYPE"/>
            <TEMPLATE_PARAM SOURCE="self.SCHEMA_STMT" TARGET="tCOB.SCHEMA_STMT"/>
            <TEMPLATE_PARAM SOURCE="self.INSERT_STMT" TARGET="tCOB.INSERT_STMT"/>

            <TEMPLATE_PARAM SOURCE="self.DB_VERSION" TARGET="tCBE.DB_VERSION"/>
            <TEMPLATE_PARAM SOURCE="self.HOST" TARGET="tCBE.HOST"/>
            <TEMPLATE_PARAM SOURCE="self.PORT" TARGET="tCBE.PORT"/>
            <TEMPLATE_PARAM SOURCE="self.REQUIRED_AUTHENTICATION" TARGET="tCBE.REQUIRED_AUTHENTICATION"/>
            <TEMPLATE_PARAM SOURCE="self.USERNAME" TARGET="tCBE.USERNAME"/>
            <TEMPLATE_PARAM SOURCE="self.PASSWORD" TARGET="tCBE.PASSWORD"/>
            <TEMPLATE_PARAM SOURCE="self.USE_HTTPS" TARGET="tCBE.USE_HTTPS"/>
            <TEMPLATE_PARAM SOURCE="self.HTTPS_SETTING" TARGET="tCBE.HTTPS_SETTING"/>
            <TEMPLATE_PARAM SOURCE="self.USE_CONF_FILE" TARGET="tCBE.USE_CONF_FILE"/>
            <TEMPLATE_PARAM SOURCE="self.CONF_FILE" TARGET="tCBE.CONF_FILE"/>
            <TEMPLATE_PARAM SOURCE="self.KEY_SPACE" TARGET="tCBE.KEY_SPACE"/>
            <TEMPLATE_PARAM SOURCE="self.COLUMN_FAMILY" TARGET="tCBE.COLUMN_FAMILY"/>
            <TEMPLATE_PARAM SOURCE="self.SSTABLE_STORE_PATH" TARGET="tCBE.SSTABLE_STORE_PATH"/>
        </TEMPLATES>
    </CODEGENERATION>
    <RETURNS/>
</COMPONENT>
