<COMPONENT>
	<HEADER PLATEFORM="ALL" SERIAL="" VERSION="0.101" STATUS="ALPHA"
		COMPATIBILITY="ALL" AUTHOR="Talend" RELEASE_DATE="20070210A"
		STARTABLE="true">
		<SIGNATURE />
	</HEADER>

	<FAMILIES>
		<FAMILY>Internet/MapR Streams</FAMILY>
	</FAMILIES>

	<DOCUMENTATION>
		<URL />
	</DOCUMENTATION>

	<CONNECTORS>
		<CONNECTOR CTYPE="FLOW" MAX_INPUT="0" MAX_OUTPUT="0" />
		<CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" MAX_OUTPUT="1" />
		<CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
		<CONNECTOR CTYPE="COMPONENT_OK" />
		<CONNECTOR CTYPE="COMPONENT_ERROR" />
		<CONNECTOR CTYPE="RUN_IF" />
	</CONNECTORS>

	<PARAMETERS>
		<PARAMETER NAME="PROPERTY" FIELD="PROPERTY_TYPE" SHOW="true"
			NUM_ROW="1" REPOSITORY_VALUE="HDFS" />

		<PARAMETER NAME="MAPRSTREAMS" FIELD="HADOOP_DISTRIBUTION"
			NUM_ROW="10" REQUIRED="false" GROUP="VERSION" />

		<PARAMETER NAME="FS_DEFAULT_NAME" FIELD="TEXT"
			REPOSITORY_VALUE="FS_DEFAULT_NAME" REQUIRED="true" NUM_ROW="15">
			<DEFAULT>"maprfs:///"</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="USE_MAPRTICKET" FIELD="CHECK" NUM_ROW="20"
			REPOSITORY_VALUE="USE_MAPRTICKET" GROUP="AUTHENTICATION"
			SHOW_IF="(DISTRIB[DISTRIBUTION, MAPRSTREAMS_VERSION].doSupportMapRTicket[])">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="USE_KRB" FIELD="CHECK" NUM_ROW="30"
			REQUIRED="true" REPOSITORY_VALUE="USE_KRB"
			SHOW_IF="((DISTRIBUTION=='CUSTOM') OR (DISTRIB[DISTRIBUTION, MAPRSTREAMS_VERSION].doSupportKerberos[])) AND ((isShow[USE_MAPRTICKET]) AND (USE_MAPRTICKET=='true'))"
			GROUP="AUTHENTICATION">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="NAMENODE_PRINCIPAL" FIELD="TEXT" GROUP="AUTHENTICATION"
			REPOSITORY_VALUE="NAMENODE_PRINCIPAL" REQUIRED="true" NUM_ROW="40"
			SHOW_IF="(isShow[USE_KRB] AND (USE_KRB=='true'))">
			<DEFAULT>"nn/_HOST@EXAMPLE.COM"</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="PRINCIPAL" FIELD="TEXT" NUM_ROW="50"
			REQUIRED="true" REPOSITORY_VALUE="KEYTAB_PRINCIPAL" GROUP="AUTHENTICATION"
			SHOW_IF="(isShow[USE_KRB]) AND (USE_KRB=='true')">
			<DEFAULT>"user@EXAMPLE.COM"</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="KEYTAB_PATH" FIELD="FILE" NUM_ROW="50"
			REQUIRED="true" REPOSITORY_VALUE="KEYTAB_PATH" GROUP="AUTHENTICATION"
			SHOW_IF="(isShow[USE_KRB]) AND (USE_KRB=='true')">
			<DEFAULT>"/home/user/user.keytab"</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="MAPRTICKET_USERNAME" FIELD="TEXT" GROUP="AUTHENTICATION"
			REPOSITORY_VALUE="MAPRTICKET_USERNAME" REQUIRED="true" NUM_ROW="60"
			SHOW_IF="((isShow[USE_MAPRTICKET]) AND (USE_MAPRTICKET=='true')) AND (((isShow[USE_KRB]) AND (USE_KRB=='false')) OR (!isShow[USE_KRB])) ">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="MAPRTICKET_PASSWORD" FIELD="PASSWORD"
			GROUP="AUTHENTICATION" REPOSITORY_VALUE="MAPRTICKET_PASSWORD"
			REQUIRED="true" NUM_ROW="60"
			SHOW_IF="((isShow[USE_MAPRTICKET]) AND (USE_MAPRTICKET=='true')) AND (((isShow[USE_KRB]) AND (USE_KRB=='false')) OR (!isShow[USE_KRB])) ">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="MAPRTICKET_CLUSTER" FIELD="TEXT" GROUP="AUTHENTICATION"
			REPOSITORY_VALUE="MAPRTICKET_CLUSTER" REQUIRED="true" NUM_ROW="70"
			SHOW_IF="((isShow[USE_MAPRTICKET]) AND (USE_MAPRTICKET=='true'))">
			<DEFAULT>"demo.mapr.com"</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="MAPRTICKET_DURATION" FIELD="TEXT" GROUP="AUTHENTICATION"
			REPOSITORY_VALUE="MAPRTICKET_DURATION" REQUIRED="true" NUM_ROW="80"
			SHOW_IF="((isShow[USE_MAPRTICKET]) AND (USE_MAPRTICKET=='true'))">
			<DEFAULT>86400L</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="SET_MAPR_HOME_DIR" FIELD="CHECK" NUM_ROW="90"
			REPOSITORY_VALUE="SET_MAPR_HOME_DIR" GROUP="AUTHENTICATION"
			SHOW_IF="((isShow[USE_MAPRTICKET]) AND (USE_MAPRTICKET=='true'))">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="MAPR_HOME_DIR" FIELD="TEXT" GROUP="AUTHENTICATION"
			REPOSITORY_VALUE="MAPR_HOME_DIR" REQUIRED="true" NUM_ROW="90"
			SHOW_IF="((isShow[SET_MAPR_HOME_DIR]) AND (SET_MAPR_HOME_DIR=='true'))">
			<DEFAULT>"/opt/mapr"</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="SET_HADOOP_LOGIN" FIELD="CHECK" NUM_ROW="100"
			REPOSITORY_VALUE="SET_HADOOP_LOGIN" GROUP="AUTHENTICATION"
			SHOW_IF="((isShow[USE_MAPRTICKET]) AND (USE_MAPRTICKET=='true'))">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="HADOOP_LOGIN" FIELD="TEXT" GROUP="AUTHENTICATION"
			REPOSITORY_VALUE="HADOOP_LOGIN" REQUIRED="true" NUM_ROW="100"
			SHOW_IF="((isShow[SET_HADOOP_LOGIN]) AND (SET_HADOOP_LOGIN=='true'))">
			<DEFAULT>"kerberos"</DEFAULT>
		</PARAMETER>
	</PARAMETERS>

	<ADVANCED_PARAMETERS>
		<PARAMETER NAME="HADOOP_ADVANCED_PROPERTIES"
			REPOSITORY_VALUE="HADOOP_ADVANCED_PROPERTIES" FIELD="TABLE" REQUIRED="false"
			NUM_ROW="10" NB_LINES="3">
			<ITEMS>
				<ITEM NAME="PROPERTY" />
				<ITEM NAME="VALUE" />
				<ITEM NAME="BUILDIN" SHOW_IF="false" VALUE="TRUE" />
			</ITEMS>
		</PARAMETER>
		
		<PARAMETER NAME="USE_DATANODE_HOSTNAME" FIELD="CHECK"
			REPOSITORY_VALUE="USE_DATANODE_HOSTNAME" NUM_ROW="20" REQUIRED="true"
			SHOW_IF="DISTRIB[DISTRIBUTION, DB_VERSION].doSupportUseDatanodeHostname[]">
			<DEFAULT>true</DEFAULT>
		</PARAMETER>
	</ADVANCED_PARAMETERS>
	
	<CODEGENERATION />
	<RETURNS />
</COMPONENT>
