	%>
    	<%@ include file="@{org.talend.designer.components.bigdata}/components/tCassandraOutput/tCassandraOutput_datastax_begin.javajet"%>
    <%
    }else{
    %>    
        <%@ include file="@{org.talend.designer.components.bigdata}/components/tCassandraOutput/tCassandraOutput_hector_begin.javajet"%>    	
    <%
    }
    %>