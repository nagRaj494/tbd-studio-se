		<PARAMETER NAME="USE_EXISTING_CONNECTION" FIELD="CHECK"
			NUM_ROW="2" GROUP="CONNECTION_GROUP">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="CONNECTION" FIELD="COMPONENT_LIST"
			REQUIRED="true" FILTER="tNeo4jConnection" NUM_ROW="2"
			SHOW_IF="USE_EXISTING_CONNECTION == 'true'" GROUP="CONNECTION_GROUP" />

		<PARAMETER NAME="DB_VERSION" FIELD="CLOSED_LIST"
			REPOSITORY_VALUE="DB_VERSION" NUM_ROW="3"
			SHOW_IF="USE_EXISTING_CONNECTION == 'false'" GROUP="CONNECTION_GROUP">
            <ITEMS DEFAULT="NEO4J_2_3_X">
                <ITEM NAME="NEO4J_2_3_X" VALUE="NEO4J_2_3_X" />
				<ITEM NAME="NEO4J_2_2_X" VALUE="NEO4J_2_2_X" />
				<ITEM NAME="NEO4J_2_1_X" VALUE="NEO4J_2_1_X" />
				<ITEM NAME="NEO4J_1_X_X" VALUE="NEO4J_1_X_X" />
			</ITEMS>
		</PARAMETER>

		<PARAMETER NAME="REMOTE_SERVER" FIELD="CHECK"
			REPOSITORY_VALUE="REMOTE_SERVER" NUM_ROW="4"
			SHOW_IF="USE_EXISTING_CONNECTION == 'false'" GROUP="CONNECTION_GROUP">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="DATABASE_PATH" FIELD="DIRECTORY"
			REPOSITORY_VALUE="DATABASE_PATH" REQUIRED="true" NUM_ROW="5"
			SHOW_IF="((USE_EXISTING_CONNECTION == 'false') AND (REMOTE_SERVER == 'false'))"
			GROUP="CONNECTION_GROUP">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="SERVER_URL" FIELD="TEXT" REPOSITORY_VALUE="SERVER_URL"
			REQUIRED="true" NUM_ROW="5"
			SHOW_IF="((USE_EXISTING_CONNECTION == 'false') AND (REMOTE_SERVER == 'true'))"
			GROUP="CONNECTION_GROUP">
			<DEFAULT>"http://localhost:7474/db/data"</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="SHUTDOWN_DB" FIELD="CHECK" NUM_ROW="6"
			SHOW_IF="USE_EXISTING_CONNECTION == 'true'" GROUP="CONNECTION_GROUP">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<!-- username only on remote server, but mandatory for neo4j 2.2.x -->
		<PARAMETER NAME="SET_USERNAME" FIELD="CHECK" NUM_ROW="7"
			GROUP="CONNECTION_GROUP" REPOSITORY_VALUE="SET_USERNAME"
			SHOW_IF="(USE_EXISTING_CONNECTION == 'false') AND (REMOTE_SERVER == 'true') AND ((DB_VERSION == 'NEO4J_1_X_X') OR (DB_VERSION == 'NEO4J_2_1_X'))">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="USERNAME" FIELD="TEXT" REQUIRED="true"
			NUM_ROW="8" GROUP="CONNECTION_GROUP" REPOSITORY_VALUE="USERNAME"
			SHOW_IF="(USE_EXISTING_CONNECTION == 'false') AND (REMOTE_SERVER == 'true') AND ((SET_USERNAME == 'true') OR ((DB_VERSION != 'NEO4J_1_X_X') AND (DB_VERSION != 'NEO4J_2_1_X')))">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="PASSWORD" FIELD="PASSWORD" REQUIRED="true"
			NUM_ROW="8" GROUP="CONNECTION_GROUP" REPOSITORY_VALUE="PASSWORD"
			SHOW_IF="(USE_EXISTING_CONNECTION == 'false') AND (REMOTE_SERVER == 'true') AND ((SET_USERNAME == 'true') OR ((DB_VERSION != 'NEO4J_1_X_X') AND (DB_VERSION != 'NEO4J_2_1_X')))">
		</PARAMETER>

		<PARAMETER NAME="MAP" FIELD="EXTERNAL" NUM_ROW="9">
			<DEFAULT />
		</PARAMETER>


		<PARAMETER NAME="USE_LABEL" FIELD="CHECK" NUM_ROW="10"
			SHOW_IF="(DATA_ACTION != 'DELETE') AND ((DB_VERSION != 'NEO4J_1_X_X') OR (USE_EXISTING_CONNECTION == 'true'))">
			<DEFAULT />
		</PARAMETER>
		<PARAMETER NAME="LABEL_NAME" FIELD="TEXT" NUM_ROW="10"
			SHOW_IF="(USE_LABEL == 'true') AND (DATA_ACTION != 'DELETE') AND ((DB_VERSION != 'NEO4J_1_X_X') OR (USE_EXISTING_CONNECTION == 'true'))">
			<DEFAULT>"Label"</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="DATA_ACTION" FIELD="CLOSED_LIST" NUM_ROW="11">
			<ITEMS DEFAULT="INSERT">
				<ITEM NAME="INSERT" VALUE="INSERT" />
				<ITEM NAME="INSERT_OR_UPDATE" VALUE="INSERT_OR_UPDATE" SHOW_IF="BATCH_IMPORT == 'false'" />
				<ITEM NAME="UPDATE" VALUE="UPDATE" SHOW_IF="BATCH_IMPORT == 'false'" />
				<ITEM NAME="DELETE" VALUE="DELETE" SHOW_IF="BATCH_IMPORT == 'false'" />
			</ITEMS>
		</PARAMETER>
		<PARAMETER NAME="INDEX_NAME" FIELD="TEST" NUM_ROW="16"
			SHOW_IF="DATA_ACTION != 'INSERT'" GROUP="INDEX_LOOKUP">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="INDEX_KEY" FIELD="TEXT" NUM_ROW="16"
			SHOW_IF="DATA_ACTION != 'INSERT'" GROUP="INDEX_LOOKUP">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="INDEX_VALUE" FIELD="COLUMN_LIST" NUM_ROW="16"
			SHOW_IF="DATA_ACTION != 'INSERT'" GROUP="INDEX_LOOKUP" />
		<PARAMETER NAME="SCHEMA" FIELD="SCHEMA_TYPE" NUM_ROW="21">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="VALUES" FIELD="TABLE" REQUIRED="true"
			NUM_ROW="4" NB_LINES="20" SHOW="false">
			<ITEMS BASED_ON_SCHEMA="true">
				<ITEM NAME="AUTO_INDEXED" />
				<ITEM NAME="INDEX_NAMES" />
			</ITEMS>
		</PARAMETER>

		<PARAMETER NAME="INDEXES" FIELD="TABLE" REQUIRED="false"
			NUM_ROW="21" NB_LINES="6" SHOW="false">
			<ITEMS>
				<ITEM NAME="SCHEMA_COLUMN" />
				<ITEM NAME="INDEX_NAME" />
				<ITEM NAME="KEY" />
				<ITEM NAME="VALUE" />
				<ITEM NAME="UNIQUE" />
			</ITEMS>
		</PARAMETER>
		<PARAMETER NAME="RELATIONSHIPS" FIELD="TABLE" REQUIRED="false"
			NUM_ROW="21" NB_LINES="6" SHOW="false">
			<ITEMS>
				<ITEM NAME="SCHEMA_COLUMN" />
				<ITEM NAME="TYPE" />
				<ITEM NAME="DIRECTION" />
				<ITEM NAME="INDEX_NAME" />
				<ITEM NAME="KEY" />
				<ITEM NAME="VALUE" />
			</ITEMS>
		</PARAMETER>
	</PARAMETERS>
