<COMPONENT>
	<HEADER PLATEFORM="ALL" SERIAL="" VERSION="0.102" STATUS="ALPHA"

		COMPATIBILITY="ALL" AUTHOR="Talend" RELEASE_DATE="20120802A"
		STARTABLE="false">
		<SIGNATURE />
	</HEADER>

	<FAMILIES>
		<FAMILY>ELT/Map/Hive</FAMILY>
	</FAMILIES>

	<DOCUMENTATION>
		<URL />
	</DOCUMENTATION>

	<CONNECTORS>
		<CONNECTOR CTYPE="FLOW" MAX_INPUT="0" MAX_OUTPUT="0" />

		<CONNECTOR CTYPE="TABLE" MAX_INPUT="1" />
		<CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="0" MAX_INPUT="0" />
		<CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
		<CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
		<CONNECTOR CTYPE="COMPONENT_OK" />
		<CONNECTOR CTYPE="COMPONENT_ERROR" />
		<CONNECTOR CTYPE="RUN_IF" />
	</CONNECTORS>

	<PARAMETERS>

		<PARAMETER NAME="DATA_ACTION" FIELD="CLOSED_LIST" NUM_ROW="6">
			<ITEMS DEFAULT="INSERT">
				<ITEM NAME="INSERT" VALUE="INSERT" />
				<ITEM NAME="OVERWRITE" VALUE="OVERWRITE" />
			</ITEMS>
		</PARAMETER>

		<PARAMETER NAME="SCHEMA" FIELD="SCHEMA_TYPE" NUM_ROW="7">
		</PARAMETER>

		<PARAMETER NAME="ELT_TABLE_NAME" FIELD="TEXT" NUM_ROW="14">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="ELT_SCHEMA_NAME" FIELD="TEXT" NUM_ROW="15">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="USE_DIFFERENT_TABLE" FIELD="CHECK" NUM_ROW="16"
			SHOW="true">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="DIFFERENT_TABLE_NAME" FIELD="TEXT" NUM_ROW="16"
			REQUIRED="true" SHOW_IF="USE_DIFFERENT_TABLE=='true'">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="PARQUET_NOTE" FIELD="LABEL" NUM_ROW="19"
			COLOR="255;0;0">
			<DEFAULT><![CDATA[Parquet is only supported if the distribution uses embedded Hive version 0.10 or later.]]></DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="TARGET_TABLE_IS_A_PARQUET_TABLE" FIELD="CHECK"
			NUM_ROW="20">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="PARQUET_COMPRESSION" FIELD="CLOSED_LIST"
			NUM_ROW="20" SHOW_IF="TARGET_TABLE_IS_A_PARQUET_TABLE=='true'">
			<ITEMS DEFAULT="UNCOMPRESSED">
				<ITEM NAME="UNCOMPRESSED" VALUE="UNCOMPRESSED" />
				<ITEM NAME="GZIP" VALUE="GZIP" />
				<ITEM NAME="SNAPPY" VALUE="SNAPPY" />
			</ITEMS>
		</PARAMETER>

		<PARAMETER NAME="FIELD_PARTITION" FIELD="TABLE" NUM_ROW="40"
			NB_LINES="5">
			<ITEMS>
				<ITEM NAME="COLUMN_NAME" VALUE="" />
				<ITEM NAME="COLUMN_VALUE" VALUE="" />
			</ITEMS>
		</PARAMETER>

		<PARAMETER NAME="MAPPING" FIELD="MAPPING_TYPE" NUM_ROW="99"
			SHOW="true" READONLY="true">
			<DEFAULT>hive_id</DEFAULT>
		</PARAMETER>

	</PARAMETERS>

	<ADVANCED_PARAMETERS>
	</ADVANCED_PARAMETERS>

	<CODEGENERATION>
		<IMPORTS>
		</IMPORTS>
	</CODEGENERATION>

	<RETURNS />
</COMPONENT>